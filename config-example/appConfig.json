{
  "currency_unit_symbol": "$",
  "auth": {
    "enabled": true,
    "tokenExpiresInHours": 12,
    "loginRateLimit": {
      "windowMinutes": 5,
      "maxAttempts": 20
    },
    "allowExportsForReader": true
  },
  "server": {
    "httpPort": 8000
  },
  "frontend": {
    "hostByBackend": true,
    "distPath": "./frontend",
    "fallbackToIndex": true
  }
}
